        "accordion_q8HWem": {
          "type": "accordion",
          "name": "t:names.accordion",
          "settings": {
            "icon": "caret",
            "dividers": true,
            "type_preset": "h5",
            "inherit_color_scheme": true,
            "color_scheme": "",
            "border": "none",
